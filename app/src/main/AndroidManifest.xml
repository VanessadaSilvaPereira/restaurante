<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pedidotech.apprestaurante">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/tema">
        <activity android:name=".MainActivity"
            android:label="Restaurante">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".menu"
            android:label="@string/menu"></activity>
        <activity
            android:name=".bebidas"
            android:label="@string/bebidas"></activity>
        <activity
            android:name=".saladas"
            android:label="@string/saladas"></activity>
        <activity
            android:name=".sobremesas"
            android:label="@string/sobremesas"></activity>
        <activity
            android:name=".pratoPrincipal"
            android:label="@string/pratoPrincipal"></activity>

        <activity android:name=".MeuPedidoActivity"
            android:label="@string/meuPedido">

        </activity>
    </application>

</manifest>